// comment outing this thingfrom java code 

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
    public static void printNum(int n){
        if (n==0){
            ;
            return;
            
        } 
        System.out.println(n);
        printNum(n-1);
        
    }
    
	public static void main (String[] args) throws java.lang.Exception
	{
		int n= 5 ;
		printNum( n);
		

	}
}




