import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner ( System.in);
		int t= sc.nextInt();
		
		while ( t-->0){
		    int n= sc.nextInt();
		    int count =0;
		  String a  = sc.next();
		    for(int i=0 ; i<n ; i++){
		       
		        if(a.charAt(i)=='0'){
		            count++;
		        }
		    }
		    int k=(n-count)+n+(120-2*n);
		     
		     float l= (k*100)/120;
		     
		     if ( l>=75){
		         System.out.println("YES");
		     }else {
		         System.out.println("NO");
		     }
       
		}
  sc.close();

	}
}

// time complexity:O(N);
